    "start": "nodemon .",
    "dev": "concurrently --kill-others \"yarn watch\" \"yarn start\""
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.15",
    "tslint": "^6.1.3",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "socket.io": "^4.4.1"
  }
}

{
  "name": "server",
  "version": "1.0.0",
  "description": "Server for WebRTC project",
  "main": "dist/index.js",
  "author": "Prince Narteh",
  "license": "MIT",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "watch": "tsc -w",
